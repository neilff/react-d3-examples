{
  "name": "react-d3-examples",
  "version": "0.1.0",
  "description": "Examples of how to use D3 with React",
  "main": "./src/index.js",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "clean": "rimraf ./dist",
    "cover": "istanbul cover _mocha -- --opts ./mocha.opts src/**/*.test.js",
    "dev": "webpack-dev-server --hot --progress",
    "lint": "eslint ./src",
    "start": "npm run clean; npm run build; node server.js",
    "test": "mocha --opts ./mocha.opts src/**/*.test.js",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "react",
    "redux",
    "d3"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/neilff/react-d3-examples"
  },
  "license": "MIT",
  "dependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.4",
    "basscss": "^7.0.4",
    "chai": "^3.4.0",
    "css-loader": "^0.23.0",
    "cssnext-loader": "^1.0.1",
    "d3": "^3.5.9",
    "eslint": "^1.8.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.6.3",
    "file-loader": "^0.8.4",
    "history": "^1.13.0",
    "immutable": "^3.7.5",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.2",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "postcss-loader": "^0.8.0",
    "radium": "^0.14.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.4",
    "redux-form": "^3.0.11",
    "redux-logger": "^2.0.4",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.4.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
